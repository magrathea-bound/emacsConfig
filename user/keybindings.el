;;Keybindings;;*These keybindings are for global level configuration.;;See indiviual packages for local level keymaps;;Evil Mode(use-package evil  :init  (setq evil-want-integration t)  (setq evil-want-keybinding nil)  (setq evil-want-C-u-scroll t)  (setq evil-want-C-i-jump nil)   :config  (evil-mode 1)  ;; Key Maps  ;;Leader Key  (evil-set-leader '(normal emacs) (kbd "SPC"))  ;;C-g works on vim mode too  (define-key evil-insert-state-map (kbd "C-g") 'evil-normal-state)  (define-key evil-insert-state-map (kbd "C-g") 'evil-normal-state)  (define-key evil-insert-state-map (kbd "C-h") 'evil-delete-backward-char-and-join)   ;; Use visual line motions even outside of visual-line-mode buffers  (evil-global-set-key 'motion "j" 'evil-next-visual-line)  (evil-global-set-key 'motion "k" 'evil-previous-visual-line)  ;; Change the initial state of specific modes  (evil-set-initial-state 'neotree-mode 'emacs)  (evil-set-initial-state 'messages-buffer-mode 'normal)  (evil-set-initial-state 'dashboard-mode 'normal));;Other Keybindings(global-set-key (kbd "<escape>") 'keyboard-escape-quit)    ;; Make ESC quit prompts;;Window Commands(global-set-key (kbd "S-C-n") 'other-window)    ;; Use Shift Control n to switch windows(global-set-key (kbd "S-C-h") 'evil-window-left)    ;; Use Shift Control n to switch windows(global-set-key (kbd "S-C-j") 'evil-window-down)    ;; Use Shift Control n to switch windows(global-set-key (kbd "S-C-k") 'evil-window-up)    ;; Use Shift Control n to switch windows(global-set-key (kbd "S-C-l") 'evil-window-right)    ;; Use Shift Control n to switch windows(evil-global-set-key 'normal (kbd "<leader>v") 'evil-window-vsplit)(evil-global-set-key 'normal (kbd "<leader>s") 'evil-window-split)(provide 'keybindings)